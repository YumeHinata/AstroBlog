{
  "backend": {
    "name": "github",
    "repo": "YumeHinata/AstroBlog",
    "branch": "main",
    "api_root": "https://api.github.com",
    "base_url": "https://astro.yum9.in",
    "auth_endpoint": "/auth"
  },
  "media_folder": "src/assets/images",
  "public_folder": "src/assets/images",
  "collections": [
    {
      "name": "terminal",
      "label": "terminal",
      "folder": "src/content/blog",
      "create": true,
      "fields": [
        {
          "label": "布局",
          "name": "layout",
          "widget": "hidden",
          "default": "blog"
        },
        {
          "label": "标题",
          "name": "title",
          "widget": "string"
        },
        {
          "label": "发布日期",
          "name": "date",
          "widget": "datetime"
        },
        {
          "label": "缩略图",
          "name": "thumbnail",
          "widget": "image"
        },
        {
          "label": "评分",
          "name": "rating",
          "widget": "number"
        },
        {
          "label": "正文",
          "name": "body",
          "widget": "markdown"
        }
      ]
    }
  ]
}